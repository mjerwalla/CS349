        <activity
            android:name=".Question2"
            android:label="Question" />
        <activity
            android:name=".Question3"
            android:label="Question" />
        <activity
            android:name=".Question4"
            android:label="Question" />
        <activity
            android:name=".Question5"
            android:label="Question" />
